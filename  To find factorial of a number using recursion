#include <stdio.h>

int factorial(int);

int factorial(int n) {
    if (n==1) {
        return 1;
    }
    int factorialN = factorial(n-1) * n;
    return factorialN;
}
int main() {
    int num;
    printf("Enter the number to find the factorial of: ");
    scanf("%d", &num);
    printf("The factorial of %d is %d",num, factorial(num));
    return 0;
}