#include <stdio.h>
void bs(int arr[], int n){
    for (int i=0; i<=n-2; i++){
        int min =i;
        for (int j=i+1; j<=n-1; j++){
            if (arr[j]<arr[min]){
                min=j;
            }
        }
        int temp=arr[min];
        arr[min]=arr[i];
        arr[i]=temp;
    }
}
int main() {
    int arr[] = {5, 2, 8, 1, 3};
    int n = sizeof(arr) / sizeof(arr[0]);
    bs(arr, n);
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    return 0;
}