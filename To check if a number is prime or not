#include <stdio.h>

int main() {
    printf("Enter Number: ");
    int i, num , prime=1;
    scanf("%d", &num);
    if (num==0 || num==1) {
        prime=0;
    }
    else {
        for (i=2;i<num;i++){
            if (num%i==0) {
                prime=0;
                break;
            }
        }
    }
    if (prime) {
        printf("%d is Prime Number", num);
    }
    else {
        printf("%d not a prime number",num);
    }
return 0;
}